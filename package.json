{
  "name": "@rpattersonnet/project-structure",
  "scope": "@rpattersonnet",
  "version": "0.0.0",
  "description": "Project structure foundation or template, package metadata",
  "scripts": {
    "prepare": "husky install",
    "test": "wtr",
    "test:watch": "wtr --watch",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint './'",
    "lint:prettier": "prettier --check './'",
    "format": "npm run format:eslint && npm run format:prettier",
    "format:eslint": "eslint --fix './'",
    "format:prettier": "prettier --write './'",
    "docs:api": "jsdoc -c './jsdocrc.json'"
  },
  "keywords": [
    "template",
    "structure"
  ],
  "license": "MIT",
  "author": {
    "name": "Ross Patterson",
    "email": "me@rpatterson.net",
    "url": "https://www.rpatterson.net/"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/rpatterson/project-structure.git"
  },
  "funding": {
    "type": "liberapay",
    "url": "https://liberapay.com/rpatterson/donate"
  },
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./*.js": "./src/*.js"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/eslint-parser": "7.21.8",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@web/test-runner": "0.16.1",
    "@web/test-runner-playwright": "0.10.0",
    "eslint": "8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha-no-only": "1.1.1",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-unicorn": "47.0.0",
    "eslint-plugin-xss": "0.1.12",
    "husky": "^8.0.0",
    "jsdoc": "4.0.2",
    "prettier": "2.8.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
